docker service create \
    --replicas 1 \
    --restart-condition on-failure \
    --restart-max-attempts 10 \
    --network external_nw \
    --name ug-multicast \
    --env SERVICE_BUS_URI=... \
    --env SERVICE_BUS_USER=... \
    --env SERVICE_BUS_PASSWORD=... \
    --env SERVICE_BUS_UG_QUEUE=... \
    --env SERVICE_BUS_CANVAS_QUEUE=... \
    ug-multicast:62
